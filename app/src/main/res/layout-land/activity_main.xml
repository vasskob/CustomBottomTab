<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingEnd="10dp"
    android:paddingRight="10dp"
    android:paddingStart="10dp"
    android:paddingLeft="10dp"
    tools:context="com.task.vasskob.customtab.MainActivity">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:orientation="vertical"
        android:weightSum="5">

        <ImageButton
            android:id="@+id/profile"
            style="@style/button_landscape"
            android:src="@drawable/ic_person_selector"/>

        <ImageButton
            android:id="@+id/search"
            style="@style/button_landscape"
            android:src="@drawable/ic_find_selector"/>

        <ImageButton
            android:id="@+id/camera"
            style="@style/button_landscape"
            android:src="@drawable/ic_camera_selector"/>

        <ImageButton
            android:id="@+id/notification"
            style="@style/button_landscape"
            android:src="@drawable/ic_notification_selector"/>

        <ImageButton
            android:id="@+id/home"
            style="@style/button_landscape"
            android:src="@drawable/ic_home_selector"/>


    </LinearLayout>

</RelativeLayout>
